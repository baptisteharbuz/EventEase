{
  "expo": {
    "name": "EventEase",
    "slug": "eventease",
    "owner": "baptisteharbuz",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/EventEase.png",
    "scheme": "eventease",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "Application de gestion d'événements avec authentification sécurisée",
    "privacy": "public",
    "splash": {
      "image": "./assets/EventEase.png",
      "resizeMode": "contain",
      "backgroundColor": "#ff00ee"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.eventease.app"
    },
    "android": {
      "package": "com.eventease.app",
      "adaptiveIcon": {
        "backgroundColor": "#ff00ee",
        "foregroundImage": "./assets/EventEase.png"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "output": "static",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "79b2a4d3-dd75-4187-b027-bb53f36ed070"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/79b2a4d3-dd75-4187-b027-bb53f36ed070"
    },
    "platforms": ["ios", "android"]
  }
}
